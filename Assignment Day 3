from tkinter import *
from tkinter import ttk
window = Tk()
window.title("Students Registration Screen")
window.geometry('300x300')
window.configure(background = "white")
lable1=label(window ,text= "Students Registration Screen", font=("bold",20)).grid(row =0,column=2)
FirstName = Label(window ,text = "First Name").grid(row = 0,column = 0)
LastName = Label(window ,text = "Last Name").grid(row = 1,column = 0)
CampName = Label(window ,text = "Camp Name").grid(row = 2,column = 0)
CollegeName = Label(window ,text = "College Name").grid(row = 3,column = 0)
DistrictName = Label(window ,text = "District Name").grid(row = 4,column = 0)
Email = Label(window ,text = "Email Id").grid(row = 5,column = 0)
Mobile = Label(window ,text = "Contact Number").grid(row = 6,column = 0)
Firstname1 = Entry(window).grid(row = 0,column = 1)
Lastname1 = Entry(window).grid(row = 1,column = 1)
Campname1 = Entry(window).grid(row = 2,column = 1)
Collegename1 = Entry(window).grid(row = 3,column = 1)
Districtname1 = Entry(window).grid(row = 4,column = 1)
Email1 = Entry(window).grid(row = 5,column = 1)
Mobile1 = Entry(window).grid(row = 6,column = 1)
def clicked():
    res = "Welcome to " + txt.get()
lbl.configure(text= res)
btn = ttk.Button(window ,text="Submit").grid(row=8,column=1)
window.mainloop()
